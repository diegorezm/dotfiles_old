#!/bin/bash 

touch index.html && touch style.css

echo "<!DOCTYPE html>
<html>
  <head>
    <title></title>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <link href="style.css" rel="stylesheet">
  </head>
  <body>
  
  </body>
</html>
" >> index.html

